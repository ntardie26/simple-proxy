{
  "serverPort": 3000,
  "proxySettings": {
    "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
    "removeTracking": true,
    "removeFrameOptions": true,
    "removeContentSecurityPolicy": true,
    "doNotTrack": true,
    "clearCookies": true
  },
  "advanced": {
    "rewriteUrls": true,
    "removeScripts": false,
    "timeout": 30000,
    "maxContentSize": 10485760
  },
  "logging": {
    "enabled": false,
    "level": "error",
    "file": "proxy.log"
  }
}